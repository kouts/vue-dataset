{
  "name": "vue-dataset",
  "description": "A vue 3 component to display datasets with filtering, paging and sorting capabilities!",
  "version": "0.0.0-semantic-release",
  "author": "Giannis Koutsaftakis",
  "license": "MIT",
  "repository": "https://github.com/kouts/vue-dataset",
  "packageManager": "pnpm@10.20.0",
  "type": "module",
  "keywords": [
    "vue",
    "component",
    "dataset",
    "datatable"
  ],
  "main": "./dist/vue-dataset.umd.js",
  "module": "./dist/vue-dataset.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-dataset.es.js",
      "require": "./dist/vue-dataset.umd.js"
    }
  },
  "unpkg": "dist/vue-dataset.umd.js",
  "sideEffects": [
    "./**/*.scss",
    "./vuepress-plugin-vue-example/**/*.vue",
    "./docs/**/*.vue"
  ],
  "scripts": {
    "dev": "vite",
    "serve": "vite preview",
    "build": "vite build",
    "test:unit": "vitest",
    "test:unit-coverage": "vitest run --coverage && make-coverage-badge --output-path ./coverage/badge-next.svg",
    "lint": "eslint \"**/*.{vue,ts,js}\"",
    "lint-fix": "eslint --fix \"**/*.{vue,ts,js}\"",
    "prepare": "husky",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "peerDependencies": {
    "vue": "^3.4.48"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@kouts/eslint-config": "^3.0.9",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^4.0.8",
    "@vue/compiler-sfc": "^3.5.24",
    "@vue/test-utils": "^2.4.6",
    "@vuepress/bundler-vite": "2.0.0-rc.26",
    "@vuepress/plugin-docsearch": "2.0.0-rc.118",
    "@vuepress/plugin-register-components": "2.0.0-rc.118",
    "@vuepress/theme-default": "2.0.0-rc.118",
    "bootstrap": "^4.6.2",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "jsdom": "^27.1.0",
    "lint-staged": "^16.2.6",
    "make-coverage-badge": "^1.2.0",
    "prettier": "3.6.2",
    "prismjs": "^1.30.0",
    "sass": "~1.93.3",
    "vite": "^7.2.2",
    "vite-plugin-html": "^3.2.2",
    "vitest": "^4.0.8",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3",
    "vuepress": "2.0.0-rc.26",
    "vuepress-plugin-vue-example": "^3.0.17",
    "vuex": "^4.1.0"
  }
}